my solution:
class Solution {
    public int numOfUnplacedFruits(int[] fruits, int[] baskets) {
        int counter = 0;
        for (int i = 0; i < fruits.length; i++) {
            int fruitsAmount = fruits[i];
            boolean placed = false;
            for (int j = 0; j < baskets.length; j++) {
                int basketCapacity = baskets[j];
                if (fruitsAmount <= basketCapacity) {
                    baskets[j] = 0;
                    placed = true;
                    break;
                }
            }

            if(!placed) { counter++; }
        }
    return counter;
    }
}